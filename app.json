{
  "expo": {
    "name": "Color√©",
    "slug": "colore",
    "owner": "colore_backend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arthur1235.colore",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"],
        "NSUserTrackingUsageDescription": "We use your data to provide a better ad experience."
      
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arthur1235.colore",
      "permissions": ["NOTIFICATIONS"],
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/favicon.png"
    },
   "plugins": [
    [
      "expo-notifications",
      {
        "icon": "./assets/icon.png",
        "color": "#000000"
      }
    ],
    [
      "expo-router",
      {
        "origin": "https://colore.expo.app"
      }
    ],
    "expo-font",
    "expo-secure-store"
  ],
      "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://colore.expo.app"
      },
      "eas": {
        "projectId": "69dfadaa-849f-4d31-950e-83ffbf90be7f"
      }
    }
  }
}
