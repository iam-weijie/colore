{
  "expo": {
    "name": "Color√©",
    "slug": "colore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.jpg",   
      "backgroundColor": ""
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
    [  "expo-router", {"origin":"https://colore.ca/"}]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
